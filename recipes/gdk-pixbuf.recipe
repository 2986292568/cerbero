class Recipe(recipe.Recipe):
    name = 'gdk-pixbuf'
    version = '2.24.1'
    deps = ['jpeg', 'glib', 'libpng', 'tiff', 'zlib' ]

    def prepare(self):
        if self.config.target_platform == Platform.WINDOWS:
            self.autoreconf = True
